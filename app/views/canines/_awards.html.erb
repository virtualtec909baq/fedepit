<div class="row text">
  <div class="col-md-12">    
    <div data-example-id="thumbnails-with-custom-content" class="bs-example">
      <div class="row">
        <% @canine_awards.each do |canine_award| %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <img alt="100%x200" data-src="holder.js/100%x200" style="height: 200px; width: 100%; display: block;" src="data:image/svg+xml;base64," data-holder-rendered="true">
            <div class="caption">
              <h3 id="thumbnail-label"><%= canine_award.name %><a href="#thumbnail-label" class="anchorjs-link"><span class="anchorjs-icon"></span></a></h3>
              <p><%= canine_award.description%></p>
            </div>
          </div>
        </div>
        <%end%>
      </div>
    </div>
  </div>
</div>
