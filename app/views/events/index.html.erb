<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Type event</th>
      <th>Name</th>
      <th>Description</th>
      <th>Date</th>
      <th>Place</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.type_event_id %></td>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= event.date %></td>
        <td><%= event.place %></td>
      </tr>
    <% end %>
  </tbody>
</table>

