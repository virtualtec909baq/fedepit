<%= javascript_include_tag "welcome"%>
<div class="row">
  <div id="skull"class="col-lg-12">
    <h1 class="page-header">Filtros de Busqueda  Cruce</h1>
  </div>
  <div class="modal-body">
    <button class="btn btn-primary" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">Filtros</button>
    <div class="top-buffer"></div>
    <div class="collapse" id="collapseExample">
      <div class="well">
        <%= search_form_for @search, url: admin_realizarcruce_path(:merge => true), :remote=>"true", :method => :get do |f|%>
            <div class="row">
              <div class="col-md-4">
                <%= f.select :feature_lips_eq, [["De Belfos Pendulosos", "0"],["De Belfos Apretados", "1"],["Normales", "2"]], {prompt: "labios"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_snout_2_eq, [[" Ato de Bozal", "0"],["Longitud del bozal", "1"],["En Punta", "2"],["Terminado recto", "3"], ["Con arrugas visibles",4]], {prompt: "hocico"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_cigamatico_snout_eq, [["Pronunciado", "0"],["Moderado", "1"],["Ausente", "2"],["En Punta", "3"]], {prompt: "cigomaticos"}, {class: "form-control"}%>
                <br>
              
                <%= f.select :feature_supracillares_snout_eq , [["correcta tijera", "0"],["Tijera inversa", "1"],["Prognatismo Avanzado", "2"],["Tenazas", "3"],["Enognatismo", "4"],["Cruzadas Anterior", "5"]], {prompt: "mordida"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_after_moving_eq , [["juntas de atras", "0"],["Corvenjon inmovil", "1"],["Roza corvejones", "2"],["Rectos", "3"]], {prompt: "Posteriores en movimiento"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_subsequent_static_eq , [["corvejones de vacas", "0"],["Corecto", "1"],["corvejon inmovil", "2"],["Rectos", "3"]], {prompt: "Posteriores en estatico"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_shaped_tail_eq , [["Correcta", "0"],["Partida", "1"],["En espiral", "2"],["Anudada", "3"],["De Bulidog", "4"],["Desviada", "5"]], {prompt: "forma de Cola"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_tail_length_eq , [["Correcta", "0"],["Larga", "1"],["Corta", "2"]], {prompt: "largo de la cola"}, {class: "form-control"}%>
                <br>
                <%= f.select :feature_tail_movement_eq , [["horizontal", "0"],["Vertical", "1"],["Caida Normal", "2"]], {prompt: "longuitud de cola"}, {class: "form-control"}%>
                <br>

                <%= f.select :feature_muscle_quality_eq , [["5 estrellas", "0"],["4 estrellas", "1"],[" 3 estrellas", "2"],["Insuficiente", "2"],["Gordo", "2"],["Por debajo de su peso", "2"]], {prompt: "calidas muscular"}, {class: "form-control"}%>
                <br>
              </div>
              <div class="col-md-12">
                <br>
                <%= f.submit :class =>"btn btn btn-primary btn-block"%>
              </div>
            </div>
        <%end%>
      </div>
    </div>
  </div>
</div>
<div id="merge"><%= render partial: '/admin/home/index' %></div>