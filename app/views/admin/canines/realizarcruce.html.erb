<div class="row">
	<div class="col-lg-12">
		<div class="form-group container m40px">
			<h3> Realizar Cruce</h3>
	    	<hr>
	    	<%= form_for :merge , url: admin_enviar_cruce_path, method: :post do |f| %>
				<%= f.label :name %><br>
      			<%= autocomplete_field_tag('canine_name', '', autocomplete_canine_name_admin_canines_path, :id => "merge_canine1", :class => "form-control", :name =>"merge[canine_1]")%>
				  <br>
				<%= autocomplete_field_tag('canine_name', '', autocomplete_canine_name_admin_canines_path, :id => "merge_canine2", :class => "form-control", :name =>"merge[canine_2]")%>
				<%= f.submit :class =>"btn btn-primary btn-block" %>
			<%end%>
			<hr>
		</div>
	</div>
</div>
