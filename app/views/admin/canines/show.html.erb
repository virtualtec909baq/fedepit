<div class="container">
  <div class="jumbotron">
    <div class="row">
      <div class="col-md-4">
       <%if @canine.images.last.blank? %>
         <img alt="" class="img-responsive" src="http://placehold.it/350x350">
        <%else%>
          <img alt="" class="img-responsive" src="<%= @canine.images.last.file%>">
        <%end%>
      </div>
      <div class="col-md-8">
        <div class="row">
          <div class="col-md-8">
            <span class="canine_name"><%= @canine.name%></span>
            <span></span>
          </div>
          <div class="col-md-4">
            <span class="canine_name pull-right">0.0</span>
          </div>
        </div>
        <hr>
        <div class="row">
          <div class="col-md-12">
            <span>Sexo:</span>
            <span><%= gender(@canine.gender)%></span>
          </div>
          <br>
          <div class="col-md-12">
            <span>Criadero:</span>
            <%-if !@canine.breeder_id.blank?%>
              <span><%= @canine.breeder.name_breed%> </span>
            <%end%> 
          </div>
          <br>
          <div class="col-md-12">
            <span>Fecha Nacimiento:</span>
            <span><%= @canine.birth%></span>
          </div>
          <br>
          <div class="col-md-12">
            <span>Propietario:</span>
            <%-if !@canine.breeder_id.blank?%>
              <span><%= @canine.breeder.name%> </span>
            <%end%>
          </div>
          <br>
          <div class="col-md-12">
            <span>Color:</span>
             <%-if !@canine.color_id.blank?%>
              <span><%= @canine.color.name%> </span>
            <%end%>
          </div>
          <br>
          <div class="col-md-12">
            <span>Raza:</span>
            <%-if !@canine.race_id.blank?%>
              <span><%= @canine.race.name%> </span>
            <%end%>
          </div>
          
          <div class="col-md-3">
            <a role="button" href="<%= admin_pedigree_path(@canine)%>" class="btn btn-lg btn-success btn-outlined btn-block">Ver Pedigree</a>
          </div>
          <div class="col-md-3">
            <%= link_to "Eliminar",admin_canine_path(@canine), method: :delete, data: { confirm: 'Are you sure?' }, :class =>"btn btn-lg btn-danger btn-outlined btn-block" %>

          </div>
          <div class="col-md-3">
              <%= link_to "Modificar", edit_admin_canine_path(@canine), :class =>"btn  btn-lg btn-info btn-outlined btn-block" %>
          </div>
          <div class="col-md-3">
            <%= link_to "Metter", new_admin_feature_path(canine_id: @canine.id), :class =>"btn  btn-lg btn-warning btn-outlined btn-block" %>

          </div>
        </div>
      </div>
    </div>

  </div>
