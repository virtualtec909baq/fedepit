<div class="row">
	<%= search_form_for @search, url: admin_canines_path do |f|%>	
		<div class="col-md-2">
			<label>Nombre</label>
		</div>	
		<div class="col-md-4">
			<%= f.search_field(:name_cont, class: 'form-control', placeholder: "buscar ejemplar")%>
		</div>
		<div class="col-md-2">
			<%= f.submit :class =>"btn btn btn-primary btn-block"%>
		</div>
		<div class="col-md-2">
			<%= link_to "Borrar Filtro", request.path, class:"btn btn-danger btn_white btn-block"%>
		</div>
		<div class="col-md-2">
			<%= link_to "Agregar Ejemplar", new_admin_canine_path, class:"btn btn-primary"%>
		</div>
	<%end%>
</div>

