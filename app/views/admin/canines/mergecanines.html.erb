<div class="row">
  <div class="col-md-6">
    <table class="table table-striped table-hover table-bordered" id="caninestable">
      <thead>
        <tr>
          <th>NOMBRE</th>
          <th>COI</th>
          <th>COUNT</th>
          <th>MIN GEN</th>
          <th>MAX GEN</th>
          <th>% OF BLOOD</th>
          <th>COR</th>
          <th>BREADING</th>
        </tr>
      </thead>
      <tbody>
        <%@root_canines.each do |canines|%> 
          <tr style="background-color: <%=canines[5]%>;">
            <th style="text-align: left;"><%= link_to canines[1], admin_canine_path(canines[7])%></th>
            <th><%= cor(get_ancestor_id(canines[0]))%></th>
            <th><%= canines[6]%></th>
            <th><%= canines[2]%></th>
            <th></th>
            <th><%= canines[3]%></th>
            <th></th>
            <th style="text-align: center;"><%= canines[4]%></th>
          </tr>
        <%end%>
      </tbody>
    </table>
  </div>

</div>