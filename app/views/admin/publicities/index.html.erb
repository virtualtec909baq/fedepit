<p id="notice"><%= notice %></p>

<h1>Listing Publicities</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicities.each do |publicity| %>
      <tr>
        <td><%= publicity.image %></td>
        <td><%= publicity.name %></td>
        <td><%= publicity.status %></td>
        <td><%= link_to 'Show', publicity %></td>
        <td><%= link_to 'Edit', edit_publicity_path(publicity) %></td>
        <td><%= link_to 'Destroy', publicity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publicity', new_publicity_path %>
