<p id="notice"><%= notice %></p>

<h1>Listing Category Championships</h1>

<table>
  <thead>
    <tr>
      <th>Race</th>
      <th>Age min</th>
      <th>Age max</th>
      <th>Period</th>
      <th>Variety</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_championships.each do |category_championship| %>
      <tr>
        <td><%= category_championship.race_id %></td>
        <td><%= category_championship.age_min %></td>
        <td><%= category_championship.age_max %></td>
        <td><%= category_championship.period %></td>
        <td><%= category_championship.variety_id %></td>
        <td><%= link_to 'Show', category_championship %></td>
        <td><%= link_to 'Edit', edit_category_championship_path(category_championship) %></td>
        <td><%= link_to 'Destroy', category_championship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category championship', new_category_championship_path %>
