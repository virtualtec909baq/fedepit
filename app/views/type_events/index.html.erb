<p id="notice"><%= notice %></p>

<h1>Listing Type Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_events.each do |type_event| %>
      <tr>
        <td><%= type_event.name %></td>
        <td><%= link_to 'Show', type_event %></td>
        <td><%= link_to 'Edit', edit_type_event_path(type_event) %></td>
        <td><%= link_to 'Destroy', type_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type event', new_type_event_path %>
