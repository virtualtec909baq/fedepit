<%= render partial: "home/boxgallery"%>
<div class="callout-box style3">
  <div class="container">
    <div class="callout-content">
      <div class="callout-text">
      </div>
    </div>
  </div>
</div>
<div class="city-bg">
    <div class="effect-cover"></div>
      <div class="well">
        <div class="row">
          <div class="container">
              <div class="col-md-8">
                    <h2><%= t('.who_we_are')%></h2>
                    <h4><%= t('.who_description')%></h4>
                </div>
                <div class="col-md-4 img_quienes">
                   <img src="/assets/electra112233.jpg" alt="" width="50%" height="50%" class="img-circle">
                </div>
                
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
    </div>
</div>
<div class="parallax" id="clients-home2">
    <div class="black-shadow"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 team">
            <h1 class="section-title"><%= t('.dogs')%></h1>
                <% @canines.each do |canine| %>
                <div class="col-lg-3 col-md-3 col-sm-3">
                  <div class="team-img view view-second">
                 <img alt="" src="/assets/TOSKO2.jpg">
                   <div class="mask"></div>
                    <div class="content">
                        <p><%= canine.race %> <br> <%= canine.breeder.name_breed %> <br> <%= canine.breeder.name %></p>
                        <ul class="list-unstyled list-inline list-social-icons">
                          <%= link_to t('.more'), :class => "btn btn-primary"  %>

                        </ul>
                    </div>
                  </div>
                  <p><%= canine.name %></p>
                </div> 
                <% end %>
            </div>
          </div>
     </div>
</div>
<div class="section" id="latest-blog-index">

  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="col-lg-12 text-center">
          <h1 class="section-title"><span><%= t('.last_article')%></span></h1>
        </div>
        <% @articles.each do |article| %>
        <div class="col-sm-4 latest-blog">
          <div class="latest-blog-content">
            <div class="img_holder">
             <div class="overlay"> <a href="blog.html"></a> </div>
             <%if article.img.blank?%>
               <img alt="" class="img-responsive" src="http://placehold.it/700x450">
               <%else%>
               <img alt="" class="img-responsive" src="<%= article.img%>">
               <%end%>
             <span class="onsaleblog">
              <span> <%= article.comments.count %> <i class="fa fa-comments"></i></span>
            </span>
          </div>
          <div class="blog-l-txt">
            <h3><%= article.title %></h3>
            <p><%= article.description %></p>
              <%= link_to t('.read'), article_path(article), :class => "btn btn-primary"  %>
          </div>
        </div>
      </div>   
      <% end %>    
    </div>
  </div>
  <!-- /.row -->

</div>
<!-- /.container -->

</div>
<div class="parallax-texture" id="iphone-home5">
  <div class="container">
    <div class="row">
      <div class="col-md-7">
        <img alt="" src="/assets/depth-cart.png">
        <img alt="" src="/assets/depth-cart-trasp.png" class="img-trasp animated fadeInLeftPhone">
      </div>
      <div class="col-md-5 iphone-desc">

        <ul>
          <li class="animated fadeInDown"><p><span class=" icon-strategy"></span>On-line store no code server side</p></li>
          <li class="animated fadeInDown"><p><span class=" icon-phone"></span>Your Shopping Cart in one hand</p></li>
          <li class="animated fadeInDown"><p><span class=" icon-search"></span>On-line store Html5 - AngularJs</p></li>
          <li class="animated fadeInDown"><p><span class=" icon-layers"></span>Framework MVVM</p></li>
          <li class="animated fadeInDown"><p><span class=" icon-expand"></span>It is fully extensible </p></li>
        </ul>
      </div>

    </div>
    <!-- /.container -->

  </div>

</div>

<div class="clear"></div>
<div class="home5-city-bg-bottom">
  <div class="well">
    <div class="container row-box">
      <div class="col-lg-12 col-md-12">
        <ul class="clients">
          <% @sponsors.each do |sponsor| %>
          <li data-toggle="tooltip" data-placement="top" title="" data-original-title="Diesel">
          <a href="#">
           <%if sponsor.image.blank?%>
             <img alt="" class="img-responsive" src="http://placehold.it/560x391">
            <%else%>
             <img alt="" class="img-responsive" src="<%= sponsor.image%>">
           <%end%>
          </a>
          </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>

</div>

<script src="/assets/modernizr.js" type="text/javascript"></script>
<script src="/assets/jquery.appear.js" type="text/javascript"></script>

<!--slide-->
<script src="/assets/slide/modernizr.custom.js" type="text/javascript"></script>
<script src="/assets/slide/classie.js" type="text/javascript"></script>
<script src="/assets/slide/boxesFx.js" type="text/javascript"></script>

<script src="/assets/minislide/jquery.flexslider.js" type="text/javascript"></script>

<!--Carousel Testimonial-->
<script src="/assets/owl_carousel/owl.carousel.js" type="text/javascript"></script>
<!--Porfolio-->
<script src="/assets/gallery/jquery.fancybox.js" type="text/javascript"></script>
<script src="/assets/gallery/jquery.mousewheel-3.0.6.pack.js" type="text/javascript"></script>

<!-- Filter Images-->
<script src="/assets/isotope.js" type="text/javascript"></script>

<script src="/assets/script.js" type="text/javascript"></script>
